<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="768.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.datahealthv2.controllers.CadastroPacienteController">
   <children>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0">
         <children>
            <AnchorPane  prefHeight="768.0" prefWidth="1280.0" styleClass="anchor">
               <stylesheets>
                  <URL value="@application.css" />
               </stylesheets>
               <children>
                  <ImageView fitHeight="75.0" fitWidth="342.0" layoutX="71.0" layoutY="29.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@imgs/datahealth.png" />
                     </image>
                  </ImageView>
                  <ImageView fitHeight="53.0" fitWidth="53.0" layoutX="189.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@imgs/download.png" />
                     </image>
                  </ImageView>
                  <Label layoutX="93.0" layoutY="90.0" text="Paciente">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <Label layoutX="158.0" layoutY="161.0" text="Informações Pessoais:">
                     <font>
                        <Font name="Comic Sans MS" size="24.0" />
                     </font>
                  </Label>
                  <Label layoutX="158.0" layoutY="212.0" text="Nome Completo">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txtNome" layoutX="158.0" layoutY="252.0" prefHeight="51.0" prefWidth="427.0" styleClass="inputTexto" />
                  <TextField fx:id="txtTipoSanguineo" layoutX="158.0" layoutY="481.0" prefHeight="51.0" prefWidth="427.0" styleClass="inputTexto" />
                  <TextField fx:id="txtEmail" layoutX="701.0" layoutY="252.0" prefHeight="51.0" prefWidth="427.0" styleClass="inputTexto" stylesheets="@application.css" />
                  <TextField fx:id="txtCpf" layoutX="158.0" layoutY="370.0" prefHeight="51.0" prefWidth="427.0" styleClass="inputTexto" />
                  <TextField fx:id="txtTelefone" layoutX="701.0" layoutY="370.0" prefHeight="51.0" prefWidth="427.0" styleClass="inputTexto" stylesheets="@application.css" />
                  <Button fx:id="btnCadastrar" layoutX="702.0" layoutY="686.0" mnemonicParsing="false" onAction="#CadastrarPaciente" prefHeight="51.0" prefWidth="427.0" text="Salvar">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                  </Button>
                  <ImageView accessibleRole="BUTTON" fitHeight="48.0" fitWidth="53.0" layoutX="915.0" layoutY="46.0" onMouseClicked="#clickBackScreen" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@imgs/93634.png" />
                     </image>
                  </ImageView>
                  <ComboBox fx:id="cbxMedicamento" layoutX="701.0" layoutY="481.0" prefHeight="51.0" prefWidth="427.0" />
                  <Label layoutX="701.0" layoutY="440.0" text="Medicamento">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txtSenha" layoutX="158.0" layoutY="601.0" prefHeight="51.0" prefWidth="427.0" styleClass="inputTexto" />
                  <Label layoutX="157.0" layoutY="555.0" text="Senha">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <Label layoutX="157.0" layoutY="440.0" text="Tipo Sanguíneo">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <Label layoutX="157.0" layoutY="324.0" text="CPF do Paciente">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <Label layoutX="701.0" layoutY="324.0" text="Telefone">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
                  <TextField fx:id="txtConfirmarSenha" layoutX="702.0" layoutY="601.0" prefHeight="51.0" prefWidth="427.0" styleClass="inputTexto" />
                  <Label layoutX="701.0" layoutY="555.0" text="Confirmar Senha">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
            <Label layoutX="701.0" layoutY="212.0" text="Email">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <Button layoutX="992.0" layoutY="46.0" mnemonicParsing="false" onAction="#clickLogoff" prefHeight="41.0" prefWidth="131.0" stylesheets="@application.css" text="Logoff" textFill="WHITE">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <font>
                  <Font name="Comic Sans MS" size="24.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
